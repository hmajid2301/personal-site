  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf public && rm -rf .cache",
    "coverage": "yarn test --coverage",
    "debug": "yarn clean && node --nolazy --inspect-brk node_modules/.bin/gatsby develop",
    "develop": "yarn clean && node --max-http-header-size=16385 node_modules/.bin/gatsby develop --open",
    "format": "prettier src/**/*.{js,jsx,ts,tx,tsx} --write",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tx,tsx}",
    "lint": "eslint --color --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "serve": "node --max-http-header-size=16385 node_modules/.bin/gatsby serve",
    "storybook": "NODE_ENV=production start-storybook",
    "storybook:build": "NODE_ENV=production build-storybook -c .storybook -o .out",
    "test": "jest",
    "test:watch": "jest --watch",
    "types:check": "tsc --project . --noEmit --pretty --skipLibCheck"
  },
